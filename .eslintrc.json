{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "valid-jsdoc": [2],
        "curly": [2],
        "default-case": [2],
        "dot-location": [2, "property"],
        "eqeqeq": 2,
        "no-extra-bind": 2,
        "no-eval": 2,
        "no-eq-null": 2,
        "no-lone-blocks": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-new-wrappers": 2,
        "no-new-func": 2,
        "no-self-compare": 2,
        "no-throw-literal": 2,
        "wrap-iife": [2, "outside"],
        "radix": 2,
        "strict": [2, "function"],
        "no-multi-spaces": 2,
        "yoda": [1, "always", {
            "exceptRange": true
        }],
        "no-undef-init": 2,
        "no-shadow": [2, {
            "builtinGlobals": false,
            "hoist": "functions",
            "allow": []
        }],
        "no-shadow-restricted-names": 2,
        "react/jsx-uses-vars": [2]
    },
    "env": {
        "browser": true
    },
    "plugins": [
        "babel",
        "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "js": true,
            "modules": true
        }
    },
    "extends": "eslint:recommended"
}
